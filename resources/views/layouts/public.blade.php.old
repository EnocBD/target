<script>
    $(function() {
        $('.ajax-form').on('submit', function() {
            var $this = $(this);
            var button = $this.find('button[type="submit"]').html();
            var form = $($this)[0];
            var formData = new FormData(form);
            var errors = 0;
            $this.find('.alert').addClass('d-none');
            $this.find('[data-required]').each(function() {
                if(!$(this).val()) {
                    $(this).addClass('has-error');
                    errors++;
                } else {
                    $(this).removeClass('has-error');
                }
            }).promise().done(function() {
                if(errors == 0) {
                    $this.find('button[type="submit"]').text('...').attr('disabled', 'disabled');
                    $.ajax({
                        url: $this.attr('action'),
                        type: 'POST',
                        data: formData,
                        contentType: false,
                        processData: false
                    }).done(function(response) {
                        if(response) {
                            $this.trigger('reset');
                            $this.find('.alert').html(response).addClass('alert-success').removeClass('d-none');
                        }
                    }).fail(function(response) {
                        $this.find('.alert').html('Ha ocurrido un error, por favor intentá de nuevo').addClass('alert-danger').removeClass('d-none');
                    }).always(function() {
                        $this.find('button').html(button).removeAttr('disabled');
                    });
                } else {
                    $this.find('.alert').html('Por favor completá todos los campos requeridos').addClass('alert-danger').removeClass('d-none');
                }
            });
            return false;
        });
    });
</script>
<script>
